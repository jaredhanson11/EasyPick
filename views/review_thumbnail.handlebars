<tr class='review_thumbnail expand' id='{{review._id}}'>
    <th>
    	<span class="sign"></span>
    	{{review.course.course_numbers}}: {{review.course.name}}
    </th>
</tr>
<tr>
    <td>Taken: {{review.term}} {{review.year}}</td>
</tr>
<tr>
    <td>Overall: {{review.overall_satisfaction}}/7</td>
</tr>
<tr>
    <td>Class Time: {{review.class_hrs}}/7</td>
</tr>
<tr>
    <td>Outside Time: {{review.outside_hrs}}/7</td>
</tr>
<tr>
    <td>Grading Difficulty: {{review.grading_difficulty}}/7</td>
</tr>
<tr>
    <td>Content Difficulty: {{review.content_difficulty}}/7</td>
</tr>

